            max_x = m_df['Integral of biomass [gDW*h/L]'].max()*1.1
            plt.scatter(m_df['Integral of biomass [gDW*h/L]'], m_df['Concentration [mM]'])
            x = np.linspace(0,max_x, 4)
            plt.plot(x, rate*x + popt[1])
            plt.show()
